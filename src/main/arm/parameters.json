{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/src/main/"
        },
        "createCluster": {
            "value": "${createCluster}"
        },
        "clusterName": {
            "value": "${clusterName}"
        }, 
        "clusterRGName": {
            "value": "${clusterRGName}"
        },
        "createACR": {
            "value": "${createACR}"
        },
        "acrName": {
            "value": "${acrName}"
        },
        "enableAppGWIngress": {
            "value": "${enableAppGWIngress}"
        },
        "appgwUsePrivateIP": {
            "value": "${appgwUsePrivateIP}"
        },
        "appGatewayCertificateOption": {
            "value": "${appGatewayCertificateOption}"
        },
        "appGatewaySSLCertData": {
            "value": "${appGatewaySSLCertData}"
        },
        "appGatewaySSLCertPassword": {
            "value": "${appGatewaySSLCertPassword}"
        },
        "keyVaultResourceGroup": {
            "value": "${keyVaultResourceGroup}"
        },
        "keyVaultName": {
            "value": "${keyVaultName}"
        },
        "keyVaultSSLCertDataSecretName": {
            "value": "${keyVaultSSLCertDataSecretName}"
        },
        "keyVaultSSLCertPasswordSecretName": {
            "value": "${keyVaultSSLCertPasswordSecretName}"
        },
        "servicePrincipal": {
            "value": "${servicePrincipal}"
        },
        "enableCookieBasedAffinity": {
            "value": "${enableCookieBasedAffinity}"
        },
        "deployApplication": {
            "value": "${deployApplication}"
        },
        "appImagePath": {
            "value": "${appImagePath}"
        },
        "appReplicas": {
            "value": "${appReplicas}"
        }
    }
}