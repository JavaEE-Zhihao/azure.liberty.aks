{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "_artifactsLocation": {
            "value": "${artifactsLocationBase}/src/main/"
        },
        "createCluster": {
            "value": "${createCluster}"
        },
        "clusterName": {
            "value": "${clusterName}"
        }, 
        "clusterRGName": {
            "value": "${clusterRGName}"
        },
        "createACR": {
            "value": "${createACR}"
        },
        "acrName": {
            "value": "${acrName}"
        },
        "acrRGName": {
            "value": "${acrRGName}"
        },
        "enableAppGWIngress": {
            "value": "${enableAppGWIngress}"
        },
        "appgwUsePrivateIP": {
            "value": "${appgwUsePrivateIP}"
        },
        "appGatewayCertificateOption": {
            "value": "${appGatewayCertificateOption}"
        },
        "appGatewaySSLCertData": {
            "value": "${appGatewaySSLCertData}"
        },
        "appGatewaySSLCertPassword": {
            "value": "${appGatewaySSLCertPassword}"
        },
        "keyVaultSSLCertDataSecretName": {
            "value": "${keyVaultSSLCertDataSecretName}"
        },
        "keyVaultSSLCertPasswordSecretName": {
            "value": "${keyVaultSSLCertPasswordSecretName}"
        },
        "enableCookieBasedAffinity": {
            "value": "${enableCookieBasedAffinity}"
        },
        "deployWLO": {
            "value": "${deployWLO}"
        },
        "edition": {
            "value": "${edition}"
        },
        "productEntitlementSource": {
            "value": "${productEntitlementSource}"
        },
        "deployApplication": {
            "value": "${deployApplication}"
        },
        "appImagePath": {
            "value": "${appImagePath}"
        },
        "appReplicas": {
            "value": "${appReplicas}"
        },
        "autoScaling": {
            "value": "${autoScalingParam}"
        },
        "cpuUtilizationPercentage": {
            "value": "${cpuUtilizationPercentageParam}"
        },
        "minReplicas": {
            "value": "${minReplicasParam}"
        },
        "maxReplicas": {
            "value": "${maxReplicasParam}"
        },
        "requestCPUMillicore": {
            "value": "${requestCPUMillicoreParam}"
        }
    }
}